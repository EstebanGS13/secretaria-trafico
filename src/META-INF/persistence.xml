<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SecretariaTraficoPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Multas</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.TiposVehiculos</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.TiposPersonas</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Modelos</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Marcas</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Concesionarios</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Infracciones</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Ciudades</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Autos</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Agentes</class>
    <class>co.edu.utp.isc.db.secretariatrafico.model.Personas</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/db_secretaria_trafico"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
